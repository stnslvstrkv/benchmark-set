#define MAX_SIZE_FIBO           40
//+------------------------------------------------------------------+
//| 5. Fibo                                                          |
//+------------------------------------------------------------------+
unsigned int fib(unsigned int n)
  {
   if(n<2)
      return(1);
   else
      return(fib(n-2)+fib(n-1));
  }
//+------------------------------------------------------------------+
//|                                                                  |
//+------------------------------------------------------------------+
ulong TestFibo(long &result)
  {
   int          i;
   unsigned int f=0;
   ulong        ticks=GetTickCount64();
//---
   result=0;
   for(i=0; i<MAX_SIZE_FIBO; i++)
     {
      f=fib(i);
      result+=f;
     }
//---
   return(GetTickCount64()-ticks);
  }
//+------------------------------------------------------------------+
