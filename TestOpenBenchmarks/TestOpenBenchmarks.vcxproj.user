<?xml version="1.0" encoding="utf-8"?>
<Project ToolsVersion="Current" xmlns="http://schemas.microsoft.com/developer/msbuild/2003">
  <PropertyGroup Condition="'$(Configuration)|$(Platform)'=='Debug|x64'">
    <LocalDebuggerCommand>$(ProjectDir)..\TestResults\$(TargetFileName)</LocalDebuggerCommand>
    <DebuggerFlavor>WindowsLocalDebugger</DebuggerFlavor>
    <LocalDebuggerCommandArguments>&gt;result_debug.txt</LocalDebuggerCommandArguments>
    <LocalDebuggerWorkingDirectory>$(ProjectDir)..\TestResults</LocalDebuggerWorkingDirectory>
  </PropertyGroup>
  <PropertyGroup Condition="'$(Configuration)|$(Platform)'=='Release|x64'">
    <LocalDebuggerCommandArguments>&gt;result_release.txt</LocalDebuggerCommandArguments>
    <DebuggerFlavor>WindowsLocalDebugger</DebuggerFlavor>
    <LocalDebuggerCommand>$(ProjectDir)..\TestResults\$(TargetFileName)</LocalDebuggerCommand>
    <LocalDebuggerWorkingDirectory>$(ProjectDir)..\TestResults</LocalDebuggerWorkingDirectory>
  </PropertyGroup>
  <PropertyGroup Condition="'$(Configuration)|$(Platform)'=='LLVM|x64'">
    <LocalDebuggerCommandArguments>&gt;result_llvm.txt</LocalDebuggerCommandArguments>
    <DebuggerFlavor>WindowsLocalDebugger</DebuggerFlavor>
    <LocalDebuggerCommand>$(ProjectDir)..\TestResults\$(TargetFileName)</LocalDebuggerCommand>
    <LocalDebuggerWorkingDirectory>$(ProjectDir)..\TestResults</LocalDebuggerWorkingDirectory>
  </PropertyGroup>
</Project>